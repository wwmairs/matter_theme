{{!< default}}
<div class="gh-page">
    <div class="gh-container">

        <h1>printing packages</h1>
        <div class="gh-postfeed print-packages">
        {{#foreach posts}}
            {{> "print-package-card" page=../pages.page }} {{!-- partials/print-package-card.hbs --}}
        {{/foreach}}
        </div>


        {{!-- If JS isn't enabled, fallback to standard pagination --}}
        <noscript>
            <div class="gh-pagination">
                {{pagination}}
            </div>
        </noscript>
    </div>
</div>
